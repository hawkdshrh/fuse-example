<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Dzookeeper.url=nodex.redhat.com:2181 -Dzookeeper.password=admin -Djava.net.preferIPv4Stack=true -Djavax.net.ssl.keyStore=/opt/fuse/keystore/clientx.chain.pkcs12 -Djavax.net.ssl.keyStoreType=PKCS12 -Djavax.net.ssl.keyStorePassword=changepass -Djavax.net.ssl.trustStore=/opt/fuse/keystore/trust.chain.pkcs12 -Djavax.net.ssl.trustStoreType=PKCS12 -Djavax.net.ssl.trustStorePassword=changepass -classpath %classpath org.fusebyexample.mq_fabric_client.simple.ThreadedConsumer</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Dzookeeper.url=nodex.redhat.com:2181 -Dzookeeper.password=admin -Djava.net.preferIPv4Stack=true -Djavax.net.ssl.keyStore=/opt/fuse/keystore/server.chain.pkcs12 -Djavax.net.ssl.keyStoreType=PKCS12 -Djavax.net.ssl.keyStorePassword=changepass -Djavax.net.ssl.trustStore=/opt/fuse/keystore/server.chain.pkcs12 -Djavax.net.ssl.trustStoreType=PKCS12 -Djavax.net.ssl.trustStorePassword=changepass -Djavax.net.ssl.debug=all -classpath %classpath org.fusebyexample.mq_fabric_client.simple.ThreadedConsumer</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Dzookeeper.url=nodex.redhat.com:2181 -Dzookeeper.password=admin -Djava.net.preferIPv4Stack=true -Djavax.net.ssl.keyStore=/opt/fuse/keystore/server.chain.pkcs12 -Djavax.net.ssl.keyStoreType=PKCS12 -Djavax.net.ssl.keyStorePassword=changepass -Djavax.net.ssl.trustStore=/opt/fuse/keystore/server.chain.pkcs12 -Djavax.net.ssl.trustStoreType=PKCS12 -Djavax.net.ssl.trustStorePassword=changepass -Djavax.net.ssl.debug=all -classpath %classpath org.fusebyexample.mq_fabric_client.simple.ThreadedConsumer</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
    </actions>
